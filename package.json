{
	"name": "react-hook-fetch",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"prepare": "husky install",
		"lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
		"lint:fix": "eslint --fix 'src/**/*.{jsx,ts,tsx}'",
		"format": "prettier --write ."
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx}": [
			"yarn  eslint",
			"yarn  format"
		]
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/react-hook-fetch.es.js",
			"require": "./dist/react-hook-fetch.umd.js"
		},
		"./dist/style.css": "./dist/style.css"
	},
	"main": "./dist/react-hook-fetch.umd.js",
	"module": "./dist/react-hook-fetch.es.js",
	"types": "./dist/index.d.ts",
	"peerDependencies": {
		"react": "18.2.0 || >=18.x",
		"react-dom": "18.2.0 || >=18.x"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jainChetan81/react-hook-fetch.git"
	},
	"keywords": [],
	"author": "Chetan Jain",
	"bugs": {
		"url": "https://github.com/jainChetan81/react-hook-fetch/issues"
	},
	"homepage": "https://github.com/jainChetan81/react-hook-fetch#readme",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10",
		"@typescript-eslint/eslint-plugin": "^5.33.0",
		"@typescript-eslint/parser": "^5.33.0",
		"@vitejs/plugin-react": "^3.1.0",
		"eslint": "^8.26.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"prettier": "^2.7.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"typescript": "^4.9.3",
		"vite": "^4.1.0",
		"vite-plugin-dts": "^1.7.2",
		"vite-plugin-linter": "^2.0.2",
		"vite-tsconfig-paths": "^4.0.5"
	}
}
